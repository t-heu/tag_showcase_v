{
  "name": "geradordeetiquetasv",
  "version": "0.0.1",
  "productName": "Gerador de Etiquetas V",
  "description": "Gerador de etiquetas para vitrine da loja V!",
  "main": "src/main.js",
  "license": "MIT",
  "author": {
    "name": "t-heu"
  },
  "keywords": [],
  "homepage": "./",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "geradordeetiquetasv",
        "icon": "resources/icons/app_icon",
        "ignore": ["docs", ".git", "README.md", "prettier.config.js", ".eslintrc.js"]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "geradordeetiquetasv"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "maintainer": "t-heu",
            "icon": "resources/icons/app_icon.png"
          }
        }
      ]
    }
  },
  "dependencies": {
    "ejs-electron": "^2.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "electron": "^16.0.2",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1"
  }
}
